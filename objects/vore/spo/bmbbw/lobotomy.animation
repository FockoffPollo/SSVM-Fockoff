{
	"animatedParts": {
		"stateTypes": {
			"bodyState": {
				"default": "ov_empty",
				"states": {
					"ov_empty"     : { "frames":  1, "cycle": 0.25, "mode": "end" },
					"ov_silhouette": { "frames":  1, "cycle": 0.25, "mode": "end" },
					"ov_idle"      : { "frames":  1, "cycle": 0.25, "mode": "end" },
					"ov_swallow"   : { "frames": 14, "cycle": 3.25, "mode": "transition", "transition": "ov_full" },
					"ov_full"      : { "frames":  4, "cycle": 1.50, "mode": "loop" },
					"ov_digest"    : { "frames": 13, "cycle": 3.25, "mode": "transition", "transition": "ov_burp" },
					"ov_burp"      : { "frames": 2, "cycle": 3.00, "mode": "end" },

					"cv_empty"     : { "frames":  1, "cycle": 0.25, "mode": "end" },
					"cv_silhouette": { "frames":  1, "cycle": 0.25, "mode": "end" },
					"cv_idle"      : { "frames":  1, "cycle": 0.25, "mode": "end" },
					"cv_swallow"   : { "frames": 10, "cycle": 3.00, "mode": "transition", "transition": "cv_full" },
					"cv_full"      : { "frames":  4, "cycle": 1.50, "mode": "loop" },
					"cv_digest"    : { "frames":  6, "cycle": 3.75, "mode": "end" }
				}
			}
		},
		"parts": {
			"foreground": {
				"properties": {
					"zLevel": 1
				},
				"partStates": {
					"bodyState": {
						"ov_empty"     : { "properties": { "image": "lobotomyov.png:empty.<frame>"      } },
						"ov_silhouette": { "properties": { "image": "lobotomyov.png:silhouette.<frame>" } },
						"ov_idle"      : { "properties": { "image": "lobotomyov.png:idle.<frame>"       } },
						"ov_swallow"   : { "properties": { "image": "lobotomyov.png:swallow.<frame>"    } },
						"ov_full"      : { "properties": { "image": "lobotomyov.png:full.<frame>"       } },
						"ov_digest"    : { "properties": { "image": "lobotomyov.png:digest.<frame>"     } },
						"ov_burp"      : { "properties": { "image": "lobotomyov.png:burp.<frame>"       } },

						"cv_empty"     : { "properties": { "image": "lobotomycv.png:empty.<frame>"      } },
						"cv_silhouette": { "properties": { "image": "lobotomycv.png:silhouette.<frame>" } },
						"cv_idle"      : { "properties": { "image": "lobotomycv.png:idle.<frame>"       } },
						"cv_swallow"   : { "properties": { "image": "lobotomycv.png:swallow.<frame>"    } },
						"cv_full"      : { "properties": { "image": "lobotomycv.png:full.<frame>"       } },
						"cv_digest"    : { "properties": { "image": "lobotomycv.png:digest.<frame>"     } }
					}
				}
			}
		}
	},
	"sounds": {
		"swallow"      : ["/sfx/vore/spinj/swallow.ogg" ],
		"fullintroov"  : ["/sfx/vore/spinj/fullintroov.ogg"  ],
		"fullintrocv"  : ["/sfx/vore/spinj/fullintrocv.ogg"  ],
		"fullov"       : ["/sfx/vore/spinj/fullov.ogg"  ],
		"fullcv"       : ["/sfx/vore/spinj/fullcv.ogg"  ],
		"fulloutroov"  : ["/sfx/vore/spinj/fulloutroov.ogg"  ],
		"fulloutrocv"  : ["/sfx/vore/spinj/fulloutrocv.ogg"  ],
		"digestov"     : ["/sfx/vore/spinj/digestov.ogg"],
		"digestcv"     : ["/sfx/vore/spinj/digestcv.ogg"],
		"burp"         : ["/sfx/vore/spinj/nothing.ogg"    ],
		"slip"         : ["/sfx/vore/spinj/slip.ogg"    ]
	}
}
